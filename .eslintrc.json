{
	"root": true,
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2015
	},
	"rules": {
		"indent": ["error", "tab", {
			"SwitchCase": 1
		}],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "single"],
		"semi": ["error", "never"],
		"no-console": "off",
		"no-inline-comments": "off"
	}
}